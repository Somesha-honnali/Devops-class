      if(!navigator.share) {
         var el = document.querySelector("#toolbar>button:nth-child(3)");
         el.style.display="none";
      }
      var p = GetURLParams();
      if( Object.keys(p).length>0 && p.txt!="" ) {
         area.value = decodeURIComponent(p.txt)+"\n\n"+area.value;
      }
